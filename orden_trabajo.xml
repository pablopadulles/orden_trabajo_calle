<tryton>
    <data>

        <!-- orden.trabajo.materiales viwes -->
        <record model="ir.ui.view" id="workers_view_tree">
            <field name="model">orden.trabajo.workers</field>
            <field name="type">tree</field>
            <field name="name">workers_tree</field>
        </record>

        <record model="ir.ui.view" id="materiales_view_tree">
            <field name="model">orden.trabajo.materiales</field>
            <field name="type">tree</field>
            <field name="name">ot_materiales_tree</field>
        </record>
        <record model="ir.ui.view" id="materiales_view_tree2">
            <field name="model">orden.trabajo.materiales</field>
            <field name="type">tree</field>
            <field name="name">ot_materiales_tree2</field>
        </record>

        <record model="ir.ui.view" id="materiales_view_form">
            <field name="model">orden.trabajo.materiales</field>
            <field name="type">form</field>
            <field name="name">orden_trabajo_materiales_form</field>
        </record>

        <record model="ir.ui.view" id="create_stock_shipment_out_start_form">
            <field name="model">orden.trabajo.create_stock_shipment_out.start</field>
            <field name="type">form</field>
            <field name="name">create_stock_shipment_out_start_form</field>
        </record>

        <!-- orden.trabajo viwes -->
        <record model="ir.ui.view" id="orden_trabajo_siniestro_view_tree">
            <field name="model">orden.trabajo</field>
            <field name="type">tree</field>
            <field name="name">orden_trabajo_siniestro_tree</field>
        </record>
        <record model="ir.ui.view" id="orden_trabajo_view_tree">
            <field name="model">orden.trabajo</field>
            <field name="type">tree</field>
            <field name="name">orden_trabajo_tree</field>
        </record>

        <record model="ir.ui.view" id="orden_trabajo_view_form">
            <field name="model">orden.trabajo</field>
            <field name="type">form</field>
            <field name="name">orden_trabajo_form</field>
        </record>

        <record model="ir.ui.view" id="orden_trabajo_postacion_view_form">
            <field name="model">orden.trabajo</field>
            <field name="type">form</field>
            <field name="name">orden_trabajo_postacion_form</field>
        </record>

        <record model="ir.ui.view" id="orden_trabajo_siniestro_view_form">
            <field name="model">orden.trabajo</field>
            <field name="type">form</field>
            <field name="name">orden_trabajo_siniestro_form</field>
        </record>

        <!-- orden.trabajo accions siniestro -->
        <record model="ir.action.act_window" id="act_orden_trabajo_siniestro">
            <field name="name">Ordenes de Siniestros</field>
            <field name="res_model">orden.trabajo</field>
           <field name="context"
               eval="{'type': 'siniestro'}" pyson="1"/>
           <field name="domain"
               eval="[('type', '=', 'siniestro')]"
               pyson="1"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_orden_trabajo_siniestro_domain_draft">
            <field name="name">Draft</field>
            <field name="sequence" eval="10"/>
            <field name="domain"
                eval="[('state', '=', 'draft')]" pyson="1"/>
            <field name="act_window" ref="act_orden_trabajo_siniestro"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_orden_trabajo_siniestro_domain_open">
            <field name="name">Open</field>
            <field name="sequence" eval="10"/>
            <field name="domain"
                eval="[('state', '=', 'open')]" pyson="1"/>
            <field name="act_window" ref="act_orden_trabajo_siniestro"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_orden_trabajo_siniestro_domain_all">
            <field name="name">All</field>
            <field name="sequence" eval="10"/>
            <field name="domain"
                eval="[('state', 'in', ['validated', 'start', 'done'])]" pyson="1"/>
            <field name="act_window" ref="act_orden_trabajo_siniestro"/>
        </record>

        <record model="ir.action.act_window.view" id="act_orden_trabajo_siniestro_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="orden_trabajo_siniestro_view_tree"/>
            <field name="act_window" ref="act_orden_trabajo_siniestro"/>
        </record>
        <record model="ir.action.act_window.view" id="act_orden_trabajo_siniestro_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="orden_trabajo_siniestro_view_form"/>
            <field name="act_window" ref="act_orden_trabajo_siniestro"/>
        </record>

        <menuitem sequence="20" id="menu_orden_trabajo_siniestro" action="act_orden_trabajo_siniestro"
            parent="oci.menu_orden_trabajo0"/>
            
        <!-- orden.trabajo accions postacion -->
        <record model="ir.action.act_window" id="act_orden_trabajo_postacion">
            <field name="name">Ordenes de Postaciones</field>
            <field name="res_model">orden.trabajo</field>
           <field name="context"
               eval="{'type': 'postacion'}" pyson="1"/>
           <field name="domain"
               eval="[('type', '=', 'postacion')]"
               pyson="1"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_orden_trabajo_postacion_domain_draft">
            <field name="name">Draft</field>
            <field name="sequence" eval="10"/>
            <field name="domain"
                eval="[('state', '=', 'draft')]" pyson="1"/>
            <field name="act_window" ref="act_orden_trabajo_postacion"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_orden_trabajo_postacion_domain_open">
            <field name="name">Open</field>
            <field name="sequence" eval="10"/>
            <field name="domain"
                eval="[('state', '=', 'open')]" pyson="1"/>
            <field name="act_window" ref="act_orden_trabajo_postacion"/>
        </record>
        <record model="ir.action.act_window.domain"
                id="act_orden_trabajo_postacion_domain_all">
            <field name="name">All</field>
            <field name="sequence" eval="10"/>
            <field name="domain"
                eval="[('state', 'in', ['validated', 'start', 'done', 'stop'])]" pyson="1"/>
            <field name="act_window" ref="act_orden_trabajo_postacion"/>
        </record>

        <record model="ir.action.act_window.view" id="act_orden_trabajo_postacion_view">
            <field name="sequence" eval="10"/>
            <field name="view" ref="orden_trabajo_view_tree"/>
            <field name="act_window" ref="act_orden_trabajo_postacion"/>
        </record>
        <record model="ir.action.act_window.view" id="act_orden_trabajo_postacion_view_form">
            <field name="sequence" eval="20"/>
            <field name="view" ref="orden_trabajo_postacion_view_form"/>
            <field name="act_window" ref="act_orden_trabajo_postacion"/>
        </record>

        <menuitem sequence="20" id="menu_orden_trabajo_postacion" action="act_orden_trabajo_postacion"
            parent="oci.menu_orden_trabajo0"/>


<!--     -->
        <record model="ir.action.wizard" id="wiz_orden_trabajo_agregar_materiales">
            <field name="name">Agregar Materiales</field>
            <field name="wiz_name">orden.trabajo.create_stock_shipment_out</field>
        </record>


    </data>
</tryton>
